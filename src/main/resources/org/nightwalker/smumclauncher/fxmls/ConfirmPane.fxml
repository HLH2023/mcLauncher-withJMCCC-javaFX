<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="195.0" prefWidth="294.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.nightwalker.smumclauncher.concrollers.ConfirmPaneConcroller">
   <children>
      <Label fx:id="confirmLabel" layoutX="14.0" layoutY="48.0" prefHeight="57.0" prefWidth="267.0" text="是否确认删除登录配置">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <Button fx:id="confirmDeleteButton" layoutX="35.0" layoutY="125.0" mnemonicParsing="false" prefHeight="41.0" prefWidth="79.0" text="确认">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
      <Button fx:id="deleteButton" layoutX="185.0" layoutY="125.0" mnemonicParsing="false" onAction="#cancelDelete" prefHeight="41.0" prefWidth="79.0" text="取消">
         <font>
            <Font size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
